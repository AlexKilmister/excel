{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"indent": [
			2,
			"tab"
		],
		"no-tabs": 0
	},
	"extends": [
		"eslint:recommended",
		"google"
	]
}
